{
  "name": "garmin-training-app",
  "version": "1.0.0",
  "description": "A webapp to pull and display Garmin Connect data in a more useful way",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
  "http-proxy-middleware": "^2.0.6",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": ["garmin", "fitness", "data", "visualization"],
  "author": "",
  "license": "MIT"
}

